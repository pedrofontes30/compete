
  <%= simple_form_for [:federation, competition] do |f| %>
    <%= f.input :description %>

    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :date, html5: true%>

    <%= f.input :prize %>
    <%= f.input :registration_deadline %>

    <%= f.input :registration_price %>
    <%= f.button :submit, class: 'btn btn-primary' %>
  <% end %>
