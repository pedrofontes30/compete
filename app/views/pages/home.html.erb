
<div class="landing_page">
  <div class="banner">
    <div id="carouselbanner" class="carousel slide carousel-fade" data-ride="carousel">
      <div class="carousel-inner">

        <div class="carousel-item active">
          <img src="https://i2.wp.com/deltagrade.com/wp-content/uploads/2016/09/rvcaXsyr.jpg?fit=1600%2C1066&ssl=1" class="d-block w-100" alt="surf">
          <div class="carousel-caption d-none d-md-block" >
            <%= image_tag asset_path('competeLOGO.png'), style: 'width: 355px; height: 355px;' %>
          </div>
        </div>

        <div class="carousel-item">
          <img src="https://images.unsplash.com/photo-1545809074-59472b3f5ecc?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1500&q=80"class="d-block w-100" alt="tennis">
          <div class="carousel-caption d-none d-md-block">
            <%= image_tag asset_path('competeLOGO.png'), style: 'width: 355px; height: 355px;' %>
          </div>
        </div>

        <div class="carousel-item">
          <img src="https://c.static-nike.com/a/images/w_1920,c_limit/pnbxmscuh2jwtecs4ein/how-do-i-add-a-run-in-my-nike-run-club-app-nike-help.jpg" class="d-block w-100" alt="yoga">
          <div class="carousel-caption d-none d-md-block">
            <%= image_tag asset_path('competeLOGO.png'), style: 'width: 355px; height: 355px;' %>
          </div>

        </div>
      </div>
    </div>
  </div>

  <div class="container my-5">
    <!-- <div class="cards"> -->
    <div class="row justify-content-between">
      <div class="col-4">
        <%= link_to competitions_path do %>
          <div class="w-100 card-category card-jiujitsu d-flex align-items-center justify-content-center">
            JiuJitsu
          </div>
        <% end %>
      </div>
      <div class="col-4">
        <div class="w-100 card-category card-tennis d-flex align-items-center justify-content-center">
          Tennis
        </div>
      </div>
      <div class="col-4">
        <div class="w-100 card-category card-running d-flex align-items-center justify-content-center">
          <span class="disableBlur">Running</span>
        </div>
      </div>
    </div>
  </div>
</div>


